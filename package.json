{
  "name": "agenr",
  "version": "0.9.18",
  "openclaw": {
    "extensions": [
      "dist/openclaw-plugin/index.js"
    ]
  },
  "description": "AGENt memoRy -- Memory infrastructure for AI agents",
  "type": "module",
  "bin": {
    "agenr": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts src/cli-main.ts src/openclaw-plugin/index.ts --format esm --dts",
    "dev": "tsup src/cli.ts src/cli-main.ts --format esm --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@libsql/client": "^0.17.0",
    "@mariozechner/pi-ai": "^0.55.3",
    "@sinclair/typebox": "^0.34.48",
    "chalk": "^5.6.2",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenr-ai/agenr.git"
  },
  "homepage": "https://agenr.ai",
  "keywords": [
    "ai",
    "memory",
    "agent",
    "mcp",
    "knowledge",
    "recall",
    "extract",
    "consolidation",
    "sqlite"
  ],
  "files": [
    "dist",
    "skills",
    "openclaw.plugin.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "author": "agenr-ai",
  "pnpm": {
    "overrides": {
      "fast-xml-parser": "^5.3.6"
    }
  }
}
