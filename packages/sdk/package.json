{
  "name": "@agenr/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Agent Gateway Protocol (AGP) â€” let your AI agent interact with real-world businesses",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
