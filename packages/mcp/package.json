{
  "name": "@agenr/mcp",
  "version": "0.1.0",
  "description": "MCP server for Agenr -- connect any AI agent to real-world businesses",
  "license": "MIT",
  "type": "module",
  "bin": {
    "agenr-mcp": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
