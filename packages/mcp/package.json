    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "dependencies": {
    "@agenr/sdk": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.12.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "agenr",
    "agp",
    "ai-agents",
    "commerce",
    "tools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agenr-ai/agenr.git",
    "directory": "packages/mcp"
  },
  "homepage": "https://agenr.ai"
}
