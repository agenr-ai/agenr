{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "agenr:adapter-api": [
        "src/adapter-api.ts"
      ]
    }
  },
  "include": [
    "src",
    "data"
  ],
  "exclude": [
    "data/runtime-adapters"
  ]
}
